<%- include('partials/sidebar') %>
<%list%>
<%user%>
<% list.map((person)=>{%>
    <%if(!user.block.includes(person.email)){%>
    <div id="main"><a href="/post?email=<%=person.email%>"><img id="profile"src="profiles/<%=person.profile%>"><%=person.email%></a>

        <%if(user.followin.includes(person.email)){%>
        <form action="/unfollow?email=<%= person.email %>" method="POST">
        <input type="hidden" name ="redirURL" value="/">
        <button type="submit">언팔로우</button>
        </form>
    </div>
        <% } else { %>
            <form action="/follow?email=<%= person.email %>" method="POST">
            <input type="hidden" name ="redirURL" value="/">
            <button type="submit">팔로우</button>
            </form>
            
        </div>
        <% } %>
    <% } %>
<%})%>


